/* pages/profile/profile.wxss */

.weui-grid__icon image {
    width: 28px;
    height: 28px;
    margin: 0 auto;
}


/* 地址信息栏 */

.localtionBar {
    height: 1.5rem;
    width: 100%;
    background-color: #17dbc1;
    color: azure;
    font-weight: 100;
    font-size: smaller;
}

.localtionBar .locationIcon {
    height: 1rem;
    width: 1rem;
    margin-left: 0.5rem;
    vertical-align: middle;
}

.localtionBar .locationName {
    font-weight: 400;
    margin-left: 0.2rem;
    display: inline-block;
    vertical-align: middle;
}

.localtionBar .loginStatus {
    vertical-align: middle;
    display: inline-block;
    margin-top: 0;
    margin-bottom: 0;
    font-size: 12px;
    line-height: 2.0;
    margin-left: 2.4rem;
}


/* 用户信息栏 */

.memberInfoBar {
    height: 1.5rem;
    width: 100%;
    background-color: #66e4dd;
    color: azure;
    font-weight: 100;
    font-size: smaller;
}


/* 用户信息栏 */

.memberInfoBar .memberHeadIcon {
    display: inline-block;
    height: 1.5rem;
    width: 1.5rem;
    position: relative;
    margin: 0 0 0 0.5rem;
    border-radius: 50%;
    overflow: hidden;
}


/* 用户信息栏 */

.memberInfoBar .memberName {
    height: 1.5rem;
    position: relative;
    top: -0.5rem;
    left: 0.5rem;
    width: 5rem;
    font-weight: 400;
}


/* 用户信息栏 */

.memberInfoBar .realFlag {
    height: 1.4rem;
    width: 1.4rem;
    margin-left: 1.0rem;
}


/* 积分 */

.memberInfoBar .integral {
    margin-right: 0.5rem;
    position: relative;
    left: 14rem;
    top: -1.8rem;
}


/* 用户信息栏 */

.integral .integralIcon {
    height: 1.4rem;
    width: 1.4rem;
}


/* 用户信息栏 */

.integral .integral {
    font-weight: 400;
    position: relative;
    top: -0.4rem;
    left: 0.1rem;
}


/**资产信息**/

.assetsBar {
    background-color: #17dbc1;
    height: 4.5rem;
    width: 100%;
    color: azure;
    font-weight: 100;
    font-size: smaller;
    text-align: center;
}


/**点击效果**/

.assetsBarItem {
    background-color: #17dbc1;
}


/**点击效果**/

.assetsBarItem :hover {
    background-color: #2bebeb;
}

.assetsBar .icon {
    height: 1.4rem;
    width: 1.4rem;
}

.assetsBar .desc {
    font-weight: 400;
    margin: 0rem 0 0 0.2rem;
    position: relative;
    top: -10rpx;
}

.assetsBar .info {
    font-weight: 400;
    margin: 0rem 0 0 0.2rem;
}

.myprofileheader {
    background-color: #17dbc1;
    height: 4.5rem;
    width: 100%;
    color: azure;
    font-weight: 100;
    font-size: smaller;
}

.myprofileName {
    font-weight: 400;
}

.myprofilephoto {
    display: inline-block;
    height: 2.0rem;
    width: 2.0rem;
    position: relative;
    margin: 0 0 0 0.5rem;
    border-radius: 50%;
    overflow: hidden;
}

.myprofileheader .headerIcon {
    display: inline-block;
    height: 1.4rem;
    width: 1.5rem;
    position: relative;
    margin: 0rem 0 0 0rem;
}

.setMiddle {
    margin-left: auto;
    margin-right: auto;
}

.myprofileLocation {
    background-color: #17dbc1;
    height: 1.5rem;
    width: 100%;
    color: azure;
    font-weight: 100;
    font-size: smaller;
}

.myprofileLocation .headerIcon {
    display: inline-block;
    height: 1.4rem;
    width: 1.5rem;
    position: relative;
    margin: 0rem 0 0 0rem;
}

.commodity_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.2;
    overflow: hidden;
    z-index: 1000;
    color: #fff;
}

.commodity_attr_box {
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background: #fff;
    padding-top: 20rpx;
}

.shouyintai_cells {
    background-color: #fff;
    line-height: 1.41176471;
    font-size: 17px;
}

.shouyintai_cell_hd {
    -webkit-box-flex: 2;
    -webkit-flex: 2;
    flex: 2;
}

.shouyintai_cell_bd {
    -webkit-box-flex: 5;
    -webkit-flex: 5;
    flex: 5;
}

.shouyintai_cell_ft {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}

.applyPackageCount {
    border: solid;
    border-width: thin;
    height: 85%;
    width: 100%;
    text-align: center;
    vertical-align: middle;
    display: inline-block;
    min-height: 60%;
}

.gridButton {
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    backGround-color: #fff;
    position: static;
    line-height: 1.6;
    border-style: none;
    border-radius: 0;
    border: 0;
}

.positionStatic {
    position: static;
}

.payDescCell {
    margin-bottom: 1.0rem;
}

.payDescCell .payDescCell_item {
    display: box;
    line-height: 2.0rem;
    margin: 0.2rem 0 0.4rem 0;
}

.payDescCell_item .payDescCell_item_hd {
    display: block;
}

.payDescCell_item_hd label {
    display: block;
    font-size: 14px;
    margin-left: 0.5rem;
}

.payDescCell_item .payDescCell_item_bd {
    display: block;
}

.payCell {
    border-top-style: solid;
    border-bottom-style: solid;
    border-width: thin;
    border-color: #e6e6e6;
}

.payCell_item {
    display: box;
    line-height: 2.5rem;
    margin: 0.2rem;
}

.totalPriceText {
    margin-left: 4rem;
    font-size: 14px;
    font-weight: 500;
}

.totalPriceMoney {
    margin-left: 0.2rem;
    font-size: 20px;
    font-weight: 600;
    color: #17dbc1;
}

.payButton {
    display: inline-block;
    width: 30%;
    text-align: center;
    background-color: #17dbc1;
}

.payButton label {
    color: #FFFFFF;
    font-size: 18px;
}

.outcome_dialog_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.2;
    overflow: hidden;
    z-index: 1000;
    color: rgb(18, 19, 18);
}

.outcome_dialog_box {
    display: inline-block;
    width: 80%;
    height: 740rpx;
    overflow: hidden;
    position: fixed;
    top: 180rpx;
    left: 75rpx;
    z-index: 2000;
    opacity: 1;
    background: rgb(230, 240, 229);
    box-shadow: 0 0 1rem #141414;
    /* border-top-left-radius: 15rpx;
    border-top-right-radius: 15rpx; */
    border-radius: 15rpx;
    border-radius: 15rpx;
}

.outcome_dialog_box .title {
    line-height: 75rpx;
    background: -webkit-linear-gradient(top, #3bcc53, #3bcc53);
    background: linear-gradient(to bottom, #3bcc53, #3bcc53);
    box-shadow: 0 7px 30px rgba(62, 9, 11, 0.3);
    font-size: 18px;
    color: #eaf3d9;
}

.outcome_dialog_box .title label {
    width: 100%;
    display: inline-block;
    text-align: center;
    font-size: 12px;
}

.outcome_dialog_box .yueBox {
    text-align: center;
    box-sizing: border-box;
    line-height: 70rpx;
    display: inline-block;
    width: 100%;
    color: #e1e1e1;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    background: #fff;
}

.outcome_dialog_box .yueBox ol {
    display: inline-block;
    width: 50%;
}

.outcome_dialog_box .yueBox ol li {
    display: inline-block;
    width: 100%;
    height: 50%;
    text-align: center;
}

.oldbrother {
    height: 70%;
}

.littlebrother {
    height: 30%;
}

.yue_money {
    color: darkgreen;
    font-family: Arial;
    font-size: 22px;
}

.yuan {
    font-size: 13px;
    color: darkgreen;
}

.desc {
    font-size: 13px;
    color: rgb(7, 7, 0);
    display: inline-block;
}

.outcome_dialog_box .outcomeAmount {
    box-sizing: border-box;
    line-height: 70rpx;
    display: inline-block;
    width: 100%;
    color: #e1e1e1;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    background: #fff;
}

.outcome_dialog_box .outcomeAmount .olname {
    display: inline-block;
    width: 25%;
    height: 100%;
}

.outcome_dialog_box .outcomeAmount .olinput {
    display: inline-block;
    width: 40%;
    height: 100%;
}

.outcome_dialog_box .outcomeAmount .olshowAmount {
    display: inline-block;
    width: 35%;
    height: 100%;
}

.outcome_dialog_box .outcomeAmount .olname label {
    display: inline-block;
    text-align: center;
    color: #000;
    font-size: 14px;
    margin-left: 10rpx;
}

.outcome_dialog_box .outcomeAmount .olinput input {
    display: inline-block;
    width: 85%;
    min-height: 22rpx !important;
    vertical-align: middle;
    color: #1AAD19;
    font-size: 22px;
    font-weight: bold;
}

.outcome_dialog_box .outcomeAmount .olshowAmount label {
    display: inline-block;
    width: 85%;
    min-height: 45rpx !important;
    vertical-align: middle;
    color: #1AAD19;
    font-size: 12px;
}

.outcome_dialog_box .outcomSubmit {
    box-sizing: border-box;
    line-height: 70rpx;
    display: inline-block;
    width: 100%;
    color: #e1e1e1;
    margin-top: 70rpx;
    margin-bottom: 10rpx;
    background: #fff;
}

.outcome_dialog_box .outcomSubmit label {
    display: inline-block;
    width: 100%;
    height: 100%;
    text-align: center;
    line-height: 100%;
}

.outcome_dialog_box .outcomSubmit label button {
    margin: 0;
    padding: 0;
    display: inline-block;
    width: 40%;
    vertical-align: middle;
    background: -webkit-linear-gradient(top, #3bcc53, #3bcc53);
    background: linear-gradient(to bottom, #3bcc53, #3bcc53);
    /* background: -webkit-linear-gradient(top, #3cb46a, #3cb46a);
    background: linear-gradient(to bottom, #3cb46a, #3cb46a); */
    box-shadow: 0 7px 30px rgba(62, 9, 11, 0.3);
}

.disableButton {
    background: -webkit-linear-gradient(top, #8f9691, #8f9691) !important;
    background: linear-gradient(to bottom, #8f9691, #8f9691) !important;
}

.input-placeholder {
    color: #57ed92;
    font-weight: normal;
    font-size: 16px;
}

.outcome_dialog_box .outcomeRule {
    box-sizing: border-box;
    line-height: 40rpx;
    display: inline-block;
    width: 100%;
    color: #e1e1e1;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    background: #fff;
}

.outcome_dialog_box .outcomeRule label {
    display: inline-block;
    width: 100%;
    font-size: 12px;
    margin-left: 10rpx;
    color: rgb(19, 20, 20);
}

.outcome_dialog_box .outcomeRule .ruleTitle {
    font-size: 13px;
    height: 20rpx;
    color: rgb(38, 215, 228)
}