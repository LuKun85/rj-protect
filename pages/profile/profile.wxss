/* pages/profile/profile.wxss */

.weui-grid__icon image {
  width: 28px;
  height: 28px;
  margin: 0 auto;
}

/* 地址信息栏 */

.localtionBar {
  height: 1.5rem;
  width: 100%;
  background-color: #00cd66;
  color: azure;
  font-weight: 100;
  font-size: smaller;
}

.localtionBar .locationIcon {
  height: 1rem;
  width: 1rem;
  margin-left: 0.5rem;
  vertical-align: middle;
}

.localtionBar .locationName {
  font-weight: 400;
  margin-left: 0.2rem;
  display: inline-block;
  vertical-align: middle;
}

.localtionBar .loginStatus {
  vertical-align: middle;
  display: inline-block;
  margin-top: 0;
  margin-bottom: 0;
  font-size: 12px;
  line-height: 2.0;
  margin-left: 2.4rem;
}

/* 用户信息栏 */

.memberInfoBar {
  height: 1.5rem;
  width: 100%;
  background-color: #57ed92;
  color: azure;
  font-weight: 100;
  font-size: smaller;
}

/* 用户信息栏 */

.memberInfoBar .memberHeadIcon {
  display: inline-block;
  height: 1.5rem;
  width: 1.5rem;
  position: relative;
  margin: 0 0 0 0.5rem;
  border-radius: 50%;
  overflow: hidden;
}

/* 用户信息栏 */

.memberInfoBar .memberName {
  height: 1.5rem;
  position: relative;
  top: -0.5rem;
  left: 0.5rem;
  width: 5rem;
  font-weight: 400;
}

/* 用户信息栏 */

.memberInfoBar .realFlag {
  height: 1.4rem;
  width: 1.4rem;
  margin-left: 1.0rem;
}

/* 积分 */

.memberInfoBar .integral {
  margin-right: 0.5rem;
  position: relative;
  left: 13rem;
  top: -1.8rem;
}

/* 用户信息栏 */

.integral .integralIcon {
  height: 1.4rem;
  width: 1.4rem;
}

/* 用户信息栏 */

.integral .integral {
  font-weight: 400;
  position: relative;
  top: -0.4rem;
  left: 0.1rem;
}

/**资产信息**/

.assetsBar {
  background-color: #00cd66;
  height: 4.5rem;
  width: 100%;
  color: azure;
  font-weight: 100;
  font-size: smaller;
  text-align: center;
}

/**点击效果**/

.assetsBarItem {
  background-color: #00cd66;
}

/**点击效果**/

.assetsBarItem :hover {
  background-color: #179330;
}

.assetsBar .icon {
  height: 1.4rem;
  width: 1.4rem;
}

.assetsBar .desc {
  font-weight: 400;
  margin: 0rem 0 0 0.2rem;
}

.assetsBar .info {
  font-weight: 400;
  margin: 0rem 0 0 0.2rem;
}

.myprofileheader {
  background-color: #00cd66;
  height: 4.5rem;
  width: 100%;
  color: azure;
  font-weight: 100;
  font-size: smaller;
}

.myprofileName {
  font-weight: 400;
}

.myprofilephoto {
  display: inline-block;
  height: 2.0rem;
  width: 2.0rem;
  position: relative;
  margin: 0 0 0 0.5rem;
  border-radius: 50%;
  overflow: hidden;
}

.myprofileheader .headerIcon {
  display: inline-block;
  height: 1.4rem;
  width: 1.5rem;
  position: relative;
  margin: 0rem 0 0 0rem;
}

.setMiddle {
  margin-left: auto;
  margin-right: auto;
}

.myprofileLocation {
  background-color: #00cd66;
  height: 1.5rem;
  width: 100%;
  color: azure;
  font-weight: 100;
  font-size: smaller;
}

.myprofileLocation .headerIcon {
  display: inline-block;
  height: 1.4rem;
  width: 1.5rem;
  position: relative;
  margin: 0rem 0 0 0rem;
}

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
}

.shouyintai_cells {
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 17px;
}

.shouyintai_cell_hd {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
  flex: 2;
}

.shouyintai_cell_bd {
  -webkit-box-flex: 5;
  -webkit-flex: 5;
  flex: 5;
}

.shouyintai_cell_ft {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.applyPackageCount {
  border: solid;
  border-width: thin;
  height: 85%;
  width: 100%;
  text-align: center;
  vertical-align: middle;
  display: inline-block;
  min-height: 60%;
}

.gridButton {
  margin-top: 0;
  margin-bottom: 0;
  padding-left: 0;
  padding-right: 0;
  backGround-color: #fff;
  position: static;
  line-height: 1.6;
  border-style: none;
  border-radius: 0;
  border: 0;
}

.positionStatic {
  position: static;
}



.payDescCell {
  margin-bottom: 1.0rem;
}

.payDescCell .payDescCell_item {
  display: box;
  line-height: 2.0rem;
  margin: 0.2rem 0 0.4rem 0;
}
.payDescCell_item .payDescCell_item_hd{
   display: block;
}

.payDescCell_item_hd label{
   display: block;
   font-size: 14px;
   margin-left:0.5rem;
}

.payDescCell_item .payDescCell_item_bd{
   display: block;
}

.payCell {
  border-top-style: solid;
  border-bottom-style: solid;
  border-width: thin;
  border-color: #e6e6e6;
}

.payCell_item {
  display: box;
  line-height: 2.5rem;
  margin: 0.2rem;
}

.totalPriceText {
  margin-left: 4rem;
  font-size: 14px;
  font-weight: 500;
}

.totalPriceMoney {
  margin-left: 0.2rem;
  font-size: 20px;
  font-weight: 600;
  color: #0e7b23;
}

.payButton {
  display: inline-block;
  width: 30%;
  text-align: center;
  background-color: #1AAD19;
}
.payButton label{
  color: #FFFFFF;
  font-size: 18px;
}