<!--pages/map/classifymap.wxml-->
<view class="mapView">
   <view class='map'>
  	  <map  id='classifymap' longitude="{{longitude}}" latitude="{{latitude}}" scale="17" controls="{{controls}}" bindcontroltap="controlMapTap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location="{{showLocation}}"  circles="{{circles}}" style="display:inline-block;width: 100%; height: 100%">
  </map>
  </view>
</view>